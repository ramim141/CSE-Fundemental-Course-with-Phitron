#include <stdio.h>
int main()
{
    int X;
    scanf("%d\n", &X);
    int even = 0, odd = 0, pos = 0, neg = 0;

    for (int i = 0; i <= X; i++)
    {
        scanf("%d ", &i);

        if (i % 2 == 0)

            even++;

        else

            odd++;

        if (i > 0)

            pos++;

        else if(i < 0)

                neg++;
    }
    printf("Even: %d\n", even);
    printf("Odd: %d\n", odd);
    printf("Positive: %d\n", pos);
    printf("Negative: %d\n", neg);
    return 0;
}
