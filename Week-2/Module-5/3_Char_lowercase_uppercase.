#include <stdio.h>

int main() {
    char letter;
    scanf("%c", &letter);
    
    if (letter >= 'a' && letter <= 'z') {
        // letter is lowercase, so convert it to uppercase and print
        printf("%c", letter - 'a' + 'A');
    } else if (letter >= 'A' && letter <= 'Z') {
        // letter is uppercase, so convert it to lowercase and print
        printf("%c", letter - 'A' + 'a');
    }
    
    return 0;
}
